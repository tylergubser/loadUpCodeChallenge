<h2>Login</h2>
<%= form_tag '/login', method: :post do %>
    
    <%= label_tag "email" %> 
    <%= email_field_tag "email", ""%> 
    <%= label_tag "password" %> 
    <%= password_field_tag "password", "" %> <br />
    <%= submit_tag "submit"%>
    
<% end %> <br />
<%="Not a member yet?" %>
<%= link_to "Signup", '/signup' %>